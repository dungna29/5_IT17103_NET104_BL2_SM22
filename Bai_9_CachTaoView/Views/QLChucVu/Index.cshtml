@model IEnumerable<EFCORE_DBFIRST_LIBRARY.DomainClass.ChucVu>
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-controller="QLChucVu" asp-action="Create">Create New</a>
</p>
<table class="table">
